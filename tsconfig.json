{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es5",
    "allowJs": false,
    "lib": ["es2017", "es6", "dom"],
    "moduleResolution": "node",
    "baseUrl": "src",
    "include": [
      "custom.d.ts"
    ],
    "paths": {
      "utils/*": ["utils/*"],
      "utils": ["utils"],
      "constants/*": ["constants/*"],
      "constants": ["constants"],
      "types/*": ["types/*"],
      "types": ["types"],
      "components/*": ["components/*"],
      "components": ["components"],
      "icons/*": ["icons/*"],
      "icons": ["icons"]
    },
    "jsx": "react"
  }
}
